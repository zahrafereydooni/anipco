<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="oval">
    <size
        android:width="120dp"
        android:height="120dp"/>

    <stroke
        android:width="2dp"
        android:color="#102746"
